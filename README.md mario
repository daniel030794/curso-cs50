# curso-cs50
#include <stdio.h>
#include <cs50.h>
int main(void)
{
    int a;
    do
    {
        a = get_int("altura: ");
    }
    while (a < 1 || a > 8);
    for (int i = 0; i < a; i++)
    {
        for (int j = 0; j < i + 1; j++)
        {
            printf("#");
        }
        printf("\n");
    }
}
